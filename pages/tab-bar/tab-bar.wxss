.footer2 {
    position: fixed;
    width: 100%;
    height: 100rpx;
    border: 1px solid red;
    bottom: 3%;
}

page {
    padding-bottom: 106rpx;
}

.icon {
    width: 46rpx;
    height: 46rpx;
}

.tabBar {
    width: 100%;
    position: fixed;
    bottom: 0;
    right: 0;
    padding: 20rpx 0 10rpx 0;
    text-align: center;
    margin-left: -4rpx;
    background: #fff;
    font-size: 24rpx;
    color: #666666;
    box-shadow: 0 -1rpx 1rpx rgb(212,212,212);
    display: flex;
    justify-content: space-around;
    z-index: 111111;
}
/* 适配iphoneX iphoneXS */
@media screen and (device-width:375px) and (device-height:812px){
    .tabBar {
        padding-bottom: 48rpx;
    }
    page{
        padding-bottom: 154rpx;
    }
}
/* 适配iphoneXR iphoneXSMax */
@media screen and (device-width:414px) and (device-height:896px){
    .tabBar {
      padding-bottom: 48rpx;
    }
    page{
        padding-bottom: 154rpx;
    }
}
.iconBox {
    width: 53rpx;
    height: 53rpx;
    margin: 0 auto;
}
.tabBar-item{
    width: 150rpx;
}

.tabBartext {
    color: #47c390;
}