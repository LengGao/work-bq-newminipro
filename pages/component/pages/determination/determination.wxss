/* pages/component//pages/determination/determination.wxss *//* pages/virTest/virTest.wxss */
page{
    background: #fff;
}
.vir-top-wrapper{
    /* width: 750rpx; */
    border-top: 1rpx dotted #cecece;
    padding:27rpx 0rpx ;
    display: flex;
}
.vir-top-wrapper:first-child{
    /* width: 750rpx; */
    border-top: 0rpx dotted #cecece;
   
}
#gl1 {
    text-align: center;
    width: 25rpx;
    height: 25rpx;
    position: absolute;
    top: 50%;
    border-left: 4rpx solid #FFFFFF;
    border-bottom: 4rpx solid #FFFFFF;
    -webkit-transform: translate(0, -50%) rotate(-135deg);
    transform: translate(0, -50%) rotate(45deg);
  }
  #gl1::before {
    content: "";
    position: absolute;
    top: -20rpx;
    left: -20rpx;
    bottom: -20rpx;
    right: -20rpx;
  }
#gl2{
    text-align: center;
    width: 15rpx;
    height: 15rpx;
    position: absolute;
    top: 50%;
    right: 22rpx;
    border-left: 1rpx solid #CCCCCC;
    border-bottom: 1rpx solid #cccccc;
    -webkit-transform: translate(0,-50%) rotate(-135deg);
    transform: translate(0,-50%) rotate(-135deg);
}
.determinContent{
    position: relative;
    background: #fff;
    z-index: 13333;
    width: 100%;
    top:428rpx ;
    padding:10rpx 30rpx;
    box-sizing: border-box;
    border-radius: 20rpx 20rpx 0 0 ;
}